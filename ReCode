local infjump = Instance.new("TextButton")
infjump.Size = UDim2.new(1, 0, 0.05, 0)
infjump.Position = UDim2.new(0, 0, 0.13, 0)
infjump.BackgroundColor3 = Color3.new(0, 0, 0)
